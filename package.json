{
  "dependencies": {
    "typescript": "^5.1.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "eslint": "^8.42.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.2",
    "playwright": "^1.34.3",
    "prettier": "^2.8.8"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint:fix",
      "git add"
    ]
  }
}
